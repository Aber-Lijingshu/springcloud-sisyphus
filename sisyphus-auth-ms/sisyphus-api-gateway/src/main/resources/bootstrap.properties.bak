management.security.enabled=false
spring.cloud.config.label=v1.0.0
spring.cloud.config.name=sisyphus-api-gateway
spring.application.name=sisyphus-api-gateway
spring.cloud.config.profile=${config.profile:dev}
# ----------start logging configuration
#logging.path=./logs
#logging.file=${spring.application.name}
#logging.config=classpath:log4j2.xml
# ----------end logging configuration
zuul.host.socket-timeout-millis=60000
zuul.host.connect-timeout-millis=10000
zuul.max.host.connections=10000
#security.basic.enabled=false
ribbon.ReadTimeout=60000
ribbon.SocketTimeout=60000
ribbon.ConnectTimeout=60000
eureka.instance.prefer-ip-address=true
hystrix.command.default.execution.timeout.enabled=true
hystrix.command.default.execution.isolation.thread.timeoutInMilliseconds=60000
zuul.retryable=true
ribbon.MaxAutoRetries=3
ribbon.MaxAutoRetriesNextServer=0